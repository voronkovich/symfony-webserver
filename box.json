{
    "files": ["WebServer.php", "WebServerConfig.php"],
    "directories": ["Command", "Resources"],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "psr/simple-cache",
                "symfony/http-foundation",
                "symfony/http-kernel",
                ".gitignore",
                ".md",
                "phpunit",
                "Tester",
                "Tests",
                "tests",
                "yaml"
            ],
            "in": "vendor"
        }
    ],
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "compression": "GZ",
    "git-version": "package_version",
    "main": "webserver",
    "output": "webserver.phar",
    "stub": true,
    "chmod": "0755"
}
